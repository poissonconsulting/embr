citHeader("To cite mbr in publications use:")

citEntry(entry = "misc",
  author       = as.person("Joe Thorley"),
  year         = "2018",
  title        = "mbr: Model Builder Utility Functions and Virtual Classes",
  doi          = "https://doi.org/10.5281/zenodo.1162349",
  textVersion  = paste(
  "Joe Thorley (2018)",
  "mbr: Model Builder Utility Functions and Virtual Classes.",
  "doi: https://doi.org/10.5281/zenodo.1162349."))
